{
  "name": "mastra-fortune-telling-frontend",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "deploy:pages": "npm run build && wrangler pages deploy dist",
    "deploy:preview": "npm run build && wrangler pages deploy dist --compatibility-date=2024-08-14",
    "test:local": "npm run dev",
    "test:cloudflare": "VITE_MASTRA_API_URL=https://your-worker.your-subdomain.workers.dev npm run dev"
  },
  "dependencies": {
    "@mastra/client-js": "^0.1.3",
    "framer-motion": "^11.5.4",
    "lucide-react": "^0.441.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.0.1",
    "@typescript-eslint/parser": "^8.0.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vite": "^5.4.1",
    "wrangler": "^3.78.12"
  },
  "keywords": [
    "fortune-telling",
    "ai-agent",
    "mastra",
    "sse-streaming",
    "cloudflare-workers",
    "react",
    "typescript"
  ],
  "description": "Fortune Telling Frontend with SSE streaming support for Cloudflare Workers",
  "repository": {
    "type": "git",
    "url": "https://github.com/Rexingleung/mastra-fortune-telling.git",
    "directory": "frontend"
  }
}